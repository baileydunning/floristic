ons = (type) => {\n    if (state.plantData['main_species'].distributions[type]) {\n      return state.plantData['main_species'].distributions[type].map(place => {\n        return <li key={place.id}>{place.name}</li>\n      })\n    } else {\n      return 'Unknown'\n    }\n  }\n\n  return (\n    <section>\n      {state.plantData ?\n        <section>\n          <h2>{state.plantData['scientific_name']}:</h2>\n          <h2>{state.plantData['common_name']} from the {state.plantData['family_common_name']}</h2>\n          <h3>Genus: {state.plantData.genus.name}</h3>\n          <h3>Family: {state.plantData.family.name}</h3>\n          <p><b>Observations: </b>{state.plantData.observations}</p>\n          <div className='distributions'>\n            <div className='native'>\n              <p><b>Native: </b></p>\n              <ul>\n                {listLocations('native')}\n              </ul>\n            </div>\n            <div className='introduced'>\n              <p><b>Introduced: </b></p>\n              <ul>\n                {listLocations('introduced')}\n              </ul>\n            </div>\n          </div>\n          <div>\n            {state.plantData['main_species'].edible ? <p>✓ Edible</p> : <p>✘ Inedible</p>}\n            {state.plantData['main_species'].vegetable ? <p>✓ Vegetable</p> : <p>✘ Not a Vegetable</p>}\n          </div>\n          <img src={state.plantData['image_url']} />\n        </section> :\n        <Loading />\n      }\n      <Link to='/'>Go back</Link>\n    </section>\n  )\n}\n\nexport default FeatureView","/Users/bailey/turing/3mod/projects/floristic/src/apiCalls.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/CardContainer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/PlantCard/PlantCard.js",["139","140"],"import { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom'\nimport HomeContext from '../../HomeContext'\nimport heart from '../../../images/heart.svg'\nimport heartFill from '../../../images/heart-fill.svg'\nimport photoNotAvailable from '../../../images/picture-not-available.png'\nimport './PlantCard.scss'\n\nconst PlantCard = ({ plant, isFavorite, addToFavorites, removeFromFavorites }) => {\n  const [icon, setIcon] = useState(isFavorite ? heartFill : heart)\n\n  const handleClick = () => {\n    if (!isFavorite) {\n      addToFavorites(plant)\n      setIcon(heartFill)\n    } else {\n      removeFromFavorites(plant.id)\n      setIcon(heart)\n    }\n  }\n\n  return (\n    <div className='plant-card'>\n      <div className='plant-card-inner'>\n        <div className='plant-card-front'>\n          {plant['image_url'] ?\n            <img src={plant['image_url']} alt='plant-img' /> :\n            <img src={photoNotAvailable} alt='plant-img' />\n          }\n        </div>\n        <div className='plant-card-back'>\n          <h2>{plant['scientific_name']}</h2>\n          <h3>{plant['common_name']}</h3>\n          <div className='plant-card-btns'>\n            <button\n              className='fav-btn' \n              onClick={handleClick}>\n              <img\n                src={icon}\n                alt='fav-icon'\n                className='heart-icon'\n              />\n            </button>\n            <button className='learn-link'>\n              <Link to={`/${plant['id']}`}>Learn More</Link>\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default PlantCard","/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeContext.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/Loading/Loading.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureContext.js",["141","142"],"import React, { useContext } from 'react'\n\nconst FeatureContext = useContext()\n\nexport default FeatureContext",["143","144"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/Header.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/ToggleView/ToggleView.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/SearchBar/SearchBar.js",["145"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Footer/Footer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/index.js",[],["146","147"],"/Users/bailey/turing/3mod/projects/floristic/client/src/App/App.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/FeatureView/FeatureView.js",["148"],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeView.js",["149","150","151"],"import React, { useEffect, useState, useReducer } from 'react'\nimport HomeContext from './HomeContext'\nimport { initialState, homeReducer } from './HomeReducer'\nimport Header from './Header/Header'\nimport CardContainer from './CardContainer/CardContainer'\nimport Loading from '../Loading/Loading'\nimport Footer from './Footer/Footer'\nimport { getPlantList } from '../apiCalls'\n\nconst HomeView = () => {\n  const [state, dispatch] = useReducer(homeReducer, initialState)\n  const [cardsOnDisplay, setCardsOnDisplay] = useState(state.plantList)\n  const [favorites, setFavorites] = useState([])\n  const [loading, setLoading] = useState(true)\n\n  useEffect(() => {\n    retrieveFromStorage()\n  }, [localStorage])\n\n  useEffect(() => {\n    saveToStorage()\n  }, [favorites])\n\n  useEffect(() => {\n    (state.plantList.length === 0) && fetchPlantList()\n\n    if (state.view === 'all') {\n      setCardsOnDisplay(state.plantList)\n    } else {\n      setCardsOnDisplay(favorites)\n    }\n  }, [state.view, state.plantList, state.pageNumber, favorites])\n\n  const fetchPlantList = () => {\n    setLoading(true)\n    getPlantList(state.pageNumber)\n      .then(data => {\n        handleFetch(data.data)\n        handleLinks(data.links)\n        setLoading(false)\n      })\n      .catch(error => console.log(error))\n    console.log('fetch')\n  }\n\n  const handleFetch = (data) => {\n    const action = { type: 'FETCH_DATA', plantList: data }\n    dispatch(action)\n  }\n\n  const handleLinks = (links) => {\n    const action = { type: 'FETCH_LINKS', links: links }\n    dispatch(action)\n  }\n\n  const jumpToPage = (page) => {\n    const action = { type: 'JUMP_TO_PAGE', pageNumber: page }\n    dispatch(action)\n    fetchPlantList()\n  }\n\n  const toggleView = () => {\n    const action = { type: 'TOGGLE_VIEW' }\n    dispatch(action)\n  }\n\n  const retrieveFromStorage = () => {\n    const storedFavorites = localStorage.getItem('favorites')\n    const parsedFavorites = JSON.parse(storedFavorites)\n    setFavorites(parsedFavorites)\n    saveToStorage()\n  }\n\n  const saveToStorage = () => {\n    localStorage.clear()\n    let stringifiedFavorites = JSON.stringify(favorites)\n    localStorage.setItem(`favorites`, stringifiedFavorites)\n  }\n\n  const addToFavorites = (plant) => {\n    setFavorites([...favorites, plant])\n    saveToStorage()\n  }\n\n  const removeFromFavorites = (id) => {\n    const favPlants = favorites.filter(plant => plant.id !== id)\n    setFavorites(favPlants)\n    saveToStorage()\n  }\n\n  const determineMaxPage = () => {\n    if (state.links.last) {\n      return state.links.last.split('=')[1]\n    } else {\n      return '1'\n    }\n  }\n\n  return (\n    <HomeContext.Provider value={state}>\n      <section>\n        <Header\n          handleFetch={handleFetch}\n          handleLinks={handleLinks}\n          toggleView={toggleView}\n        />\n        {!loading ?\n          <CardContainer\n            favorites={favorites}\n            cardsOnDisplay={cardsOnDisplay}\n            addToFavorites={addToFavorites}\n            removeFromFavorites={removeFromFavorites}\n          /> :\n          <Loading />\n        }\n        {state.view === 'all' && \n          <Footer\n            determineMaxPage={determineMaxPage} \n            jumpToPage={jumpToPage} \n          />\n        }\n      </section>\n    </HomeContext.Provider>\n  )\n}\n\nexport default HomeView","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeContext.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/apiCalls.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/CardContainer/CardContainer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/Header.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/Loading/Loading.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Footer/Footer.js",["152"],"import { useContext, useEffect, useState } from 'react'\nimport HomeContext from '../HomeContext'\nimport './Footer.scss'\n\nconst Footer = ({ determineMaxPage, jumpToPage }) => {\n  const [maxPage, setMaxPage] = useState(1)\n  const context = useContext(HomeContext)\n  \n  useEffect(() => {\n    let pageNum = determineMaxPage()\n    \n    if (pageNum.includes('&q')) {\n      pageNum = pageNum.split('&')[0]\n      setMaxPage(parseInt(pageNum))\n    } else {\n      setMaxPage(parseInt(pageNum))\n    }\n  })\n\n  return (\n    <footer className='footer' data-testid='footer'>\n      { context.pageNumber > 1 && \n        <button \n          onClick={() => jumpToPage(context.pageNumber -= 1)}>\n          ←\n        </button>}\n      <p>{ context.pageNumber }</p>\n      { context.pageNumber < maxPage && \n        <button \n          onClick={() => jumpToPage(context.pageNumber += 1)}>\n          →\n        </button>}\n    </footer>\n  )\n}\n\nexport default Footer","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/ToggleView/ToggleView.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/CardContainer/PlantCard/PlantCard.js",["153","154"],"import { useState } from 'react'\nimport { Link } from 'react-router-dom'\nimport heart from '../../../images/heart.svg'\nimport heartFill from '../../../images/heart-fill.svg'\nimport photoNotAvailable from '../../../images/picture-not-available.png'\nimport './PlantCard.scss'\n\nconst PlantCard = ({ plant, isFavorite, addToFavorites, removeFromFavorites }) => {\n  const [icon, setIcon] = useState(isFavorite ? heartFill : heart)\n  const [altText, setAltText] = useState(isFavorite ? 'favorite' : 'notFavorite')\n  \n  const handleClick = () => {\n    if (!isFavorite) {\n      addToFavorites(plant)\n      setIcon(heartFill)\n      setAltText('favorite')\n    } else {\n      removeFromFavorites(plant.id)\n      setIcon(heart)\n      setAltText('notFavorite')\n    }\n  }\n\n  return (\n    <div className='plant-card'>\n      <div className='plant-card-inner'>\n        <div className='plant-card-front'>\n          {plant['image_url'] ?\n            <img src={plant['image_url']} alt='image' /> :\n            <img src={photoNotAvailable} alt='image' />\n          }\n        </div>\n        <div className='plant-card-back'>\n          <h2>{plant['scientific_name']}</h2>\n          <h3>{plant['common_name']}</h3>\n          <div className='plant-card-btns'>\n            <button\n              className='fav-btn'\n              data-testid='fav-toggle' \n              onClick={handleClick}>\n              <img\n                src={icon}\n                alt={altText}\n                className='heart-icon'\n              />\n            </button>\n            <button className='learn-link'>\n              <Link to={`/${plant['id']}`}>Learn More</Link>\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default PlantCard","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/SearchBar/SearchBar.js",["155"],"import { useContext, useState } from 'react'\nimport { searchPlants } from '../../../apiCalls'\nimport './SearchBar.scss'\n\nconst SearchBar = ({ handleFetch, handleLinks }) => {\n  const [userInput, setUserInput] = useState('')\n\n  const fetchQueryResults = (event) => {\n    event.preventDefault()\n    searchPlants(userInput)\n      .then(data => {\n        handleFetch(data.data)\n        handleLinks(data.links)\n        setUserInput('')\n      })\n      .catch(error => console.log(error))\n  }\n\n  return (\n    <form\n      className='search-form'\n      onSubmit={(event) => fetchQueryResults(event)}>\n      <label>search for plants by name, location, or whatever you want.\n        <input\n          type='text'\n          className='search-input'\n          placeholder='search...'\n          onChange={(event) => setUserInput(event.target.value)}\n          value={userInput}\n        />\n        <input\n          className='submit'\n          type='submit'\n          value='GO'\n        />\n      </label>\n    </form>\n  )\n}\n\nexport default SearchBar",{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","severity":1,"message":"161","line":18,"column":6,"nodeType":"162","endLine":18,"endColumn":20,"suggestions":"163"},{"ruleId":"160","severity":1,"message":"164","line":22,"column":6,"nodeType":"162","endLine":22,"endColumn":17,"suggestions":"165"},{"ruleId":"160","severity":1,"message":"166","line":32,"column":6,"nodeType":"162","endLine":32,"endColumn":64,"suggestions":"167"},{"ruleId":"168","severity":1,"message":"169","line":61,"column":11,"nodeType":"170","endLine":61,"endColumn":53},{"ruleId":"171","severity":1,"message":"172","line":1,"column":20,"nodeType":"173","messageId":"174","endLine":1,"endColumn":30},{"ruleId":"171","severity":1,"message":"175","line":3,"column":8,"nodeType":"173","messageId":"174","endLine":3,"endColumn":19},{"ruleId":"171","severity":1,"message":"176","line":1,"column":8,"nodeType":"173","messageId":"174","endLine":1,"endColumn":13},{"ruleId":"177","severity":2,"message":"178","line":3,"column":24,"nodeType":"173","endLine":3,"en