     getPlant(id)\n        .then(data => handleFetch(data.data))\n        .catch(error => console.log(error))\n    }\n  }, [id, state.plantData])\n\n  const handleFetch = (data) => {\n    const action = { type: 'FETCH_DATA', plantData: data }\n    dispatch(action)\n  }\n\n  const listLocations = (type) => {\n    if (state.plantData['main_species'].distributions[type]) {\n      return state.plantData['main_species'].distributions[type].map(place => {\n        return <li key={place.id}>{place.name}</li>\n      })\n    } else {\n      return 'Unknown'\n    }\n  }\n\n  return (\n    <section>\n      {state.plantData ?\n        <section>\n          <h2>{state.plantData['scientific_name']}:</h2>\n          <h2>{state.plantData['common_name']} from the {state.plantData['family_common_name']}</h2>\n          <h3>Genus: {state.plantData.genus.name}</h3>\n          <h3>Family: {state.plantData.family.name}</h3>\n          <p><b>Observations: </b>{state.plantData.observations}</p>\n          <div className='distributions'>\n            <div className='native'>\n              <p><b>Native: </b></p>\n              <ul>\n                {listLocations('native')}\n              </ul>\n            </div>\n            <div className='introduced'>\n              <p><b>Introduced: </b></p>\n              <ul>\n                {listLocations('introduced')}\n              </ul>\n            </div>\n          </div>\n          <div>\n            {state.plantData['main_species'].edible ? <p>✓ Edible</p> : <p>✘ Inedible</p>}\n            {state.plantData['main_species'].vegetable ? <p>✓ Vegetable</p> : <p>✘ Not a Vegetable</p>}\n          </div>\n          <img src={state.plantData['image_url']} />\n        </section> :\n        <Loading />\n      }\n      <Link to='/'>Go back</Link>\n    </section>\n  )\n}\n\nexport default FeatureView","/Users/bailey/turing/3mod/projects/floristic/src/apiCalls.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/CardContainer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/PlantCard/PlantCard.js",["143","144"],"import { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom'\nimport HomeContext from '../../HomeContext'\nimport heart from '../../../images/heart.svg'\nimport heartFill from '../../../images/heart-fill.svg'\nimport photoNotAvailable from '../../../images/picture-not-available.png'\nimport './PlantCard.scss'\n\nconst PlantCard = ({ plant, isFavorite, addToFavorites, removeFromFavorites }) => {\n  const [icon, setIcon] = useState(isFavorite ? heartFill : heart)\n\n  const handleClick = () => {\n    if (!isFavorite) {\n      addToFavorites(plant)\n      setIcon(heartFill)\n    } else {\n      removeFromFavorites(plant.id)\n      setIcon(heart)\n    }\n  }\n\n  return (\n    <div className='plant-card'>\n      <div className='plant-card-inner'>\n        <div className='plant-card-front'>\n          {plant['image_url'] ?\n            <img src={plant['image_url']} alt='plant-img' /> :\n            <img src={photoNotAvailable} alt='plant-img' />\n          }\n        </div>\n        <div className='plant-card-back'>\n          <h2>{plant['scientific_name']}</h2>\n          <h3>{plant['common_name']}</h3>\n          <div className='plant-card-btns'>\n            <button\n              className='fav-btn' \n              onClick={handleClick}>\n              <img\n                src={icon}\n                alt='fav-icon'\n                className='heart-icon'\n              />\n            </button>\n            <button className='learn-link'>\n              <Link to={`/${plant['id']}`}>Learn More</Link>\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default PlantCard","/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeContext.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/Loading/Loading.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureContext.js",["145","146"],"import React, { useContext } from 'react'\n\nconst FeatureContext = useContext()\n\nexport default FeatureContext",["147","148"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/Header.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/ToggleView/ToggleView.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/SearchBar/SearchBar.js",["149"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Footer/Footer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/index.js",[],["150","151"],"/Users/bailey/turing/3mod/projects/floristic/client/src/App/App.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/FeatureView/FeatureView.js",["152","153"],"import { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport Loading from '../Loading/Loading'\nimport { getPlant } from '../apiCalls'\nimport './FeatureView.scss'\n\nconst FeatureView = ({ id }) => {\n  const [plantData, setPlantData] = useState(null)\n  const [error, setError] = useState(null)\n\n  useEffect(() => {\n    if (!plantData) {\n      getPlant(id)\n        .then(data => setPlantData(data.data))\n        .catch(err => setError(err))\n    }\n  }, [id, plantData])\n\n  const listLocations = (type) => {\n    if (plantData['main_species'].distributions[type]) {\n      return plantData['main_species'].distributions[type].map(place => {\n        return <li key={place.id}>{place.name}</li>\n      })\n    } else {\n      return 'Unknown'\n    }\n  }\n\n  return (\n    <section className='feature' data-testid='feature-view'>\n      {plantData ?\n        <section>\n          <h1>{plantData['scientific_name']}:</h1>\n          <h2>{plantData['common_name']} from the {plantData['family_common_name']}</h2>\n          <h3>Genus: {plantData.genus.name}</h3>\n          <h3>Family: {plantData.family.name}</h3>\n          <p><b>Observations: </b>{plantData.observations}</p>\n          <div className='distributions'>\n            <div className='native'>\n              <p><b>Native: </b></p>\n              <ul>\n                {listLocations('native')}\n              </ul>\n            </div>\n            <div className='introduced'>\n              <p><b>Introduced: </b></p>\n              <ul>\n                {listLocations('introduced')}\n              </ul>\n            </div>\n          </div>\n          <div>\n            {plantData['main_species'].edible ? <p>✓ Edible</p> : <p>✘ Inedible</p>}\n            {plantData['main_species'].vegetable ? <p>✓ Vegetable</p> : <p>✘ Not a Vegetable</p>}\n          </div>\n          <img src={plantData['image_url']} />\n        </section> :\n        <Loading />\n      }\n      <Link to='/'>Go back</Link>\n    </section>\n  )\n}\n\nexport default FeatureView","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeView.js",["154","155","156","157"],"import React, { useEffect, useState, useReducer } from 'react'\nimport HomeContext from './HomeContext'\nimport { initialState, homeReducer } from './HomeReducer'\nimport Header from './Header/Header'\nimport CardContainer from './CardContainer/CardContainer'\nimport Loading from '../Loading/Loading'\nimport Footer from './Footer/Footer'\nimport { getPlantList, searchPlants } from '../apiCalls'\n\nconst HomeView = ({ query }) => {\n  const [state, dispatch] = useReducer(homeReducer, initialState)\n  const [cardsOnDisplay, setCardsOnDisplay] = useState(state.plantList)\n  const [favorites, setFavorites] = useState([])\n  const [search, setSearch] = useState(query)\n  const [loading, setLoading] = useState(true)\n  const [error, setError] = useState(null)\n  \n  useEffect(() => {\n    retrieveFromStorage()\n  }, [localStorage])\n\n  useEffect(() => {\n    saveToStorage()\n  }, [favorites])\n\n  useEffect(() => {\n    (state.plantList.length === 0) && fetchPlantList()\n\n    if (state.view === 'all') {\n      setCardsOnDisplay(state.plantList)\n    } else {\n      setCardsOnDisplay(favorites)\n    }\n  }, [state.view, state.plantList, state.pageNumber, favorites])\n\n  const fetchPlantList = () => {\n    setLoading(true)\n    if (!search) {\n      getPlantList(state.pageNumber)\n        .then(data => {\n          handleFetch(data.data)\n          handleLinks(data.links)\n          setLoading(false)\n        })\n        .catch(err => setError(err))\n    } else {\n      searchPlants(search)\n        .then(data => {\n          handleFetch(data.data)\n          handleLinks(data.links)\n          setLoading(false)\n        })\n        .catch(err => setError(err))\n    }\n  }\n\n  const handleFetch = (data) => {\n    const action = { type: 'FETCH_DATA', plantList: data }\n    dispatch(action)\n  }\n\n  const handleLinks = (links) => {\n    const action = { type: 'FETCH_LINKS', links: links }\n    dispatch(action)\n  }\n\n  const jumpToPage = (page) => {\n    const action = { type: 'JUMP_TO_PAGE', pageNumber: page }\n    dispatch(action)\n    fetchPlantList()\n  }\n\n  const toggleView = () => {\n    const action = { type: 'TOGGLE_VIEW' }\n    dispatch(action)\n  }\n\n  const retrieveFromStorage = () => {\n    const storedFavorites = localStorage.getItem('favorites')\n    const parsedFavorites = JSON.parse(storedFavorites)\n    setFavorites(parsedFavorites)\n    saveToStorage()\n  }\n\n  const saveToStorage = () => {\n    localStorage.clear()\n    let stringifiedFavorites = JSON.stringify(favorites)\n    localStorage.setItem(`favorites`, stringifiedFavorites)\n  }\n\n  const addToFavorites = (plant) => {\n    setFavorites([...favorites, plant])\n    saveToStorage()\n  }\n\n  const removeFromFavorites = (id) => {\n    const favPlants = favorites.filter(plant => plant.id !== id)\n    setFavorites(favPlants)\n    saveToStorage()\n  }\n\n  const updateSearch = (queryRequest) => {\n    setSearch(queryRequest)\n    fetchPlantList()\n  }\n\n  const determineMaxPage = () => {\n    if (state.links.last) {\n      return state.links.last.split('=')[1]\n    } else {\n      return '1'\n    }\n  }\n\n  return (\n    <HomeContext.Provider value={state}>\n      <section>\n        <Header\n          toggleView={toggleView}\n          updateSearch={updateSearch}\n        />\n        {!loading ?\n          <CardContainer\n            favorites={favorites}\n            cardsOnDisplay={cardsOnDisplay}\n            addToFavorites={addToFavorites}\n            removeFromFavorites={removeFromFavorites}\n          /> :\n          <Loading />\n        }\n        {state.view === 'all' && \n          <Footer\n            determineMaxPage={determineMaxPage} \n            jumpToPage={jumpToPage} \n          />\n        }\n      </section>\n    </HomeContext.Provider>\n  )\n}\n\nexport default HomeView","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeContext.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/HomeReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/apiCalls.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/CardContainer/CardContainer.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/Header.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/Loading/Loading.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Footer/Footer.js",["158"],"import { useContext, useEffect, useState } from 'react'\nimport HomeContext from '../HomeContext'\nimport './Footer.scss'\n\nconst Footer = ({ determineMaxPage, jumpToPage }) => {\n  const [maxPage, setMaxPage] = useState(1)\n  const context = useContext(HomeContext)\n  \n  useEffect(() => {\n    let pageNum = determineMaxPage()\n    \n    if (pageNum.includes('&q')) {\n      pageNum = pageNum.split('&')[0]\n      setMaxPage(parseInt(pageNum))\n    } else {\n      setMaxPage(parseInt(pageNum))\n    }\n  })\n\n  return (\n    <footer className='footer' data-testid='footer'>\n      { context.pageNumber > 1 && \n        <button \n          onClick={() => jumpToPage(context.pageNumber -= 1)}>\n          ←\n        </button>}\n      <p>{ context.pageNumber }</p>\n      { context.pageNumber < maxPage && \n        <button \n          onClick={() => jumpToPage(context.pageNumber += 1)}>\n          →\n        </button>}\n    </footer>\n  )\n}\n\nexport default Footer","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/ToggleView/ToggleView.js",[],"/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/CardContainer/PlantCard/PlantCard.js",["159","160"],"import { useState } from 'react'\nimport { Link } from 'react-router-dom'\nimport heart from '../../../images/heart.svg'\nimport heartFill from '../../../images/heart-fill.svg'\nimport photoNotAvailable from '../../../images/picture-not-available.png'\nimport './PlantCard.scss'\n\nconst PlantCard = ({ plant, isFavorite, addToFavorites, removeFromFavorites }) => {\n  const [icon, setIcon] = useState(isFavorite ? heartFill : heart)\n  const [altText, setAltText] = useState(isFavorite ? 'favorite' : 'notFavorite')\n  \n  const handleClick = () => {\n    if (!isFavorite) {\n      addToFavorites(plant)\n      setIcon(heartFill)\n      setAltText('favorite')\n    } else {\n      removeFromFavorites(plant.id)\n      setIcon(heart)\n      setAltText('notFavorite')\n    }\n  }\n\n  return (\n    <div className='plant-card'>\n      <div className='plant-card-inner'>\n        <div className='plant-card-front'>\n          {plant['image_url'] ?\n            <img src={plant['image_url']} alt='image' /> :\n            <img src={photoNotAvailable} alt='image' />\n          }\n        </div>\n        <div className='plant-card-back'>\n          <h2>{plant['scientific_name']}</h2>\n          <h3>{plant['common_name']}</h3>\n          <div className='plant-card-btns'>\n            <button\n              className='fav-btn'\n              data-testid='fav-toggle' \n              onClick={handleClick}>\n              <img\n                src={icon}\n                alt={altText}\n                className='heart-icon'\n              />\n            </button>\n            <button className='learn-link'>\n              <Link to={`/plant/${plant['id']}`}>Learn More</Link>\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default PlantCard","/Users/bailey/turing/3mod/projects/floristic/client/src/HomeView/Header/SearchBar/SearchBar.js",["161","162"],"/Users/bailey/turing/3mod/projects/floristic/client/src/Error/Error.js",[],{"ruleId":"163","replacedBy":"164"},{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","severity":1,"message":"168","line":18,"column":6,"nodeType":"169","endLine":18,"endColumn":20,"suggestions":"170"},{"ruleId":"167","severity":1,"message":"171","line":22,"column":6,"nodeType":"169","endLine":22,"endColumn":17,"suggestions":"172"},{"ruleId":"167","severity":1,"message":"173","line":32,"column":6,"nodeType":"169","endLine":32,"endColumn":64,"suggestions":"174"},{"ruleId":"175","severity":1,"message":"176","line":61,"column":11,"nodeType":"177","endLine":61,"endColumn":53},{"ruleId":"178","severity":1,"message":"179","line":1,"column":20,"nodeType":"180","messageId":"181","endLine":1,"endColumn":30},{"ruleId":"178","severity":1,"message":"182","line":3,"column":8,"nodeType":"180","messageId":"181","endLine":3,"endColumn":19},{"ruleId":"178","severity":1,"message":"183","line":1,"column":8,"nodeType":"180","messageId":"181","endLine":1,"endColumn":13},{"ruleId":"184","severity":2,"message":"185","line":3,"column":24,"nodeType":"180","endLine":3,"endColumn":34},{"ruleId":"163","replacedBy":"186"},{"ruleId":"165","replacedBy":"187"},{"ruleId":"178","severity":1,"message":"179","line":1,"column":10,"nodeType":"180","messageId":"181","endLine":1,"endColumn":20},{