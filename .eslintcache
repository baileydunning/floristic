 }\n  }\n\n  return (\n    <section>\n      {state.plantData ?\n        <section>\n          <h2>{state.plantData['scientific_name']}:</h2>\n          <h2>{state.plantData['common_name']} from the {state.plantData['family_common_name']}</h2>\n          <h3>Genus: {state.plantData.genus.name}</h3>\n          <h3>Family: {state.plantData.family.name}</h3>\n          <p><b>Observations: </b>{state.plantData.observations}</p>\n          <div className='distributions'>\n            <div className='native'>\n              <p><b>Native: </b></p>\n              <ul>\n                {listLocations('native')}\n              </ul>\n            </div>\n            <div className='introduced'>\n              <p><b>Introduced: </b></p>\n              <ul>\n                {listLocations('introduced')}\n              </ul>\n            </div>\n          </div>\n          <div>\n            {state.plantData['main_species'].edible ? <p>✓ Edible</p> : <p>✘ Inedible</p>}\n            {state.plantData['main_species'].vegetable ? <p>✓ Vegetable</p> : <p>✘ Not a Vegetable</p>}\n          </div>\n          <img src={state.plantData['image_url']} />\n        </section> :\n        <Loading />\n      }\n      <Link to='/'>Go back</Link>\n    </section>\n  )\n}\n\nexport default FeatureView","/Users/bailey/turing/3mod/projects/floristic/src/apiCalls.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/CardContainer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/CardContainer/PlantCard/PlantCard.js",["78","79"],"import { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom'\nimport HomeContext from '../../HomeContext'\nimport heart from '../../../images/heart.svg'\nimport heartFill from '../../../images/heart-fill.svg'\nimport photoNotAvailable from '../../../images/picture-not-available.png'\nimport './PlantCard.scss'\n\nconst PlantCard = ({ plant, isFavorite, addToFavorites, removeFromFavorites }) => {\n  const [icon, setIcon] = useState(heart)\n  \n  const handleClick = () => {\n    if (!isFavorite) {\n      addToFavorites(plant)\n      setIcon(heartFill)\n    } else {\n      removeFromFavorites(plant.id)\n      setIcon(heart)\n    }\n  }\n\n  return (\n    <div className='plant-card'>\n      <div className='plant-card-inner'>\n        <div className='plant-card-front'>\n          {plant['image_url'] ? \n            <img src={ plant['image_url'] } alt='plant-img'/> :\n            <img src={ photoNotAvailable } alt='plant-img' />\n          }\n        </div>\n        <div className='plant-card-back'>\n          <h2>{ plant['scientific_name'] }</h2>\n          <h3>{ plant['common_name'] }</h3>\n          <div>\n            <button onClick={handleClick}>\n              <img \n                src={icon}\n                alt='fav-icon'\n                className='heart-icon' \n              />\n            </button>\n          </div>\n          <button>\n            <Link to={`/${plant['id']}`}>Learn More</Link>\n          </button>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default PlantCard",["80","81"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeContext.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/HomeReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/Loading/Loading.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureReducer.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/FeatureView/FeatureContext.js",["82","83"],"import React, { useContext } from 'react'\n\nconst FeatureContext = useContext()\n\nexport default FeatureContext",["84","85"],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/Header.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/ToggleView/ToggleView.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Header/SearchBar/SearchBar.js",[],"/Users/bailey/turing/3mod/projects/floristic/src/HomeView/Footer/Footer.js",[],